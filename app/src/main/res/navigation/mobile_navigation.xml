<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_login"
        android:name="com.jonsung.goeunj.ui.login.LoginFragment"
        android:label="@string/tab_login"
        tools:layout="@layout/fragment_login">


    </fragment>

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.jonsung.goeunj.ui.home.HomeFragment"
        android:label="@string/tab_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_workout"
        android:name="com.jonsung.goeunj.ui.workout_plan.WorkoutPlanFragment"
        android:label="@string/tab_workout"
        tools:layout="@layout/fragment_workout" />

    <fragment
        android:id="@+id/navigation_progress"
        android:name="com.jonsung.goeunj.ui.progress.ProgressFragment"
        android:label="@string/tab_progress"
        tools:layout="@layout/fragment_progress" />

</navigation>